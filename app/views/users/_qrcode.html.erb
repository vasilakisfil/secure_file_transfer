<div class="row">
  <div class="small-6 small-centered columns text-center">
    <table id="qrcode">
    <% @qr.modules.each_index do |x| %>
        <tr>
          <% @qr.modules.each_index do |y| %>
              <% if @qr.dark?(x,y) %>
                  <td class="black"/>
              <% else %>
                  <td class="white"/>
              <% end %>
          <% end %>
        </tr>
    <% end %>
    </table>
  </div>
</div>

